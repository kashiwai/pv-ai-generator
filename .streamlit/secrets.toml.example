# Streamlit Cloud Secrets Template
# Copy this to Streamlit Cloud Settings -> Secrets

# PIAPI統合（Hailuo, Midjourney等を含む）
PIAPI_KEY = "your-piapi-main-key"  # メインKEY（認証用）
PIAPI_XKEY = "your-piapi-x-key"     # XKEY（サービスアクセス用）
PIAPI_BASE_URL = "https://api.piapi.ai"

# その他の必須APIキー
OPENAI_API_KEY = "sk-..."
GOOGLE_API_KEY = "your-google-key"

# オプションAPIキー
ANTHROPIC_API_KEY = "sk-ant-..."
FISH_AUDIO_API_KEY = "your-fish-audio-key"
DEEPSEEK_API_KEY = "your-deepseek-key"
EOF < /dev/null